{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
      semi: [1, "never"],
      indent: [1, 4],
      "max-len": [1, 125],
      "react/jsx-indent-props": [1, 4],
      "react/jsx-indent": [1, 4],
      "react/prop-types": 0
  }
}
